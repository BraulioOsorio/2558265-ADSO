<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_preguntas_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_preguntas_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cuestionario"
        tools:targetApi="31">
        <activity
            android:name=".Responder_preguntas"
            android:exported="false" />
        <activity
            android:name=".Iniciar_cuestionario"
            android:exported="false" />
        <activity
            android:name=".DetalleCuestionario"
            android:exported="false" />
        <activity
            android:name=".ResumenUsuario"
            android:exported="false"
            android:label="@string/title_activity_resumen_usuario"
            android:theme="@style/Theme.Cuestionario" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>